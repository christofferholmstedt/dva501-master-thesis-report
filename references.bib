    Keywords = {UPPAAL, real-time, model-checker, development, COMPUTER SCIENCE, SOFTWARE ENGINEERING},
    Number = {2},
    Pages = {133 - 142},
    Title = {Developing UPPAAL over 15 years.},
    Volume = {41},
    URL = {http://ep.bib.mdh.se/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=edswsc&AN=000287106000002&site=eds-live},
    Year = {2011},
}
